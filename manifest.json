{
	"name": "Pasting",
	"description": "Chrome extension to create quick Pastes with Pasting.io",
	"version": "0.1",
	"manifest_version": 2,
  	"permissions": [ 
  		"tabs",
  		"https://ajax.googleapis.com/",
    	"http://api.pasting.io/",
    	"*://*.pasting.io/",
    	"cookies"
  	],
  	"browser_action": {
     	"default_icon": "assets/256.png",
     	"default_popup": "pastingExtension.html"
  	},
 	"icons": {
        "16": "assets/16.png",
        "48": "assets/48.png",
        "128": "assets/128.png",
        "256": "assets/256.png"
    }
}
